{
  "compilerOptions": {
    "target": "ES2022",
    "lib": ["es2022", "WebWorker"],
    "module": "es2022",
    "moduleResolution": "bundler",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "types": ["@cloudflare/workers-types"]
  },
  "include": ["src/**/*", "test/**/*", "vitest.config.ts"]
}
